{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "google",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "no-tabs": 0,
        "valid-jsdoc": "off",
        "require-jsdoc": "off",
        "object-curly-spacing": "off",
        "comma-dangle": "off",
        "max-len": "off",
        "guard-for-in": "off",
        "space-before-function-paren":"off"
    }
}